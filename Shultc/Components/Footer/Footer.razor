@ChildContent
<div class="footer-wrapper">
    <div class="footer-nav">
        <div class="navlink-footer">
            <NavLink href="#home">Главная</NavLink>
        </div>
        <div class="navlink-footer">
            <NavLink href="about#about">Автобиография</NavLink>
        </div>
        <div class="navlink-footer">
            <NavLink href="missions#missions">Цели и задачи</NavLink>
        </div>
        <div class="navlink-footer">
            <NavLink href="projects#projects">Проекты</NavLink>
        </div>
        <div class="navlink-footer">
            <NavLink href="achievements#achievements">Достижения</NavLink>
        </div>
        <div class="navlink-footer">
            <NavLink href="reception#reception">Виртуальная приёмная</NavLink>
        </div>
    </div>
    <div class="footer-description">
        <span>Пишите обращения в виртуальную приёмную по ссылке: <NavLink href="reception">Виртуальная приёмная</NavLink></span>
    </div>
    <div class="tmp">
        <button class="btn btn-primary tmpb1" @onclick="@(e=>ButtonClickCallback.InvokeAsync(sourseSans))">sourseSans</button>
        <button class="btn btn-primary tmpb2" @onclick="@(e=>ButtonClickCallback.InvokeAsync(commissioner))">commissioner</button>
        <button class="btn btn-primary tmpb3" @onclick="@(e=>ButtonClickCallback.InvokeAsync(inter))">inter</button>
    </div>
</div>
@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }
    private string? fontFamily = "";
    private string? sourseSans = "sourceSans3";
    private string? commissioner = "commissioner";
    private string? inter = "inter";
    [Parameter]
    public EventCallback<string> ButtonClickCallback { get; set; }
}
